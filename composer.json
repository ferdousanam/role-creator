{
    "name": "sudippalash/role-creator",
    "description": "Laravel package for Role Management",
    "keywords": [
        "Sudip",
        "Laravel Role Management",
        "Role Management",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudip Palash",
            "email": "palash.sudip@gmail.com"
        }
